// prisma/schema.prisma

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
}

// O nosso modelo (representa a tabela)
model User {
  id         Int    @id @default(autoincrement()) @map("_id")
  nome       String
  tecnologia String
  email      String @unique
  age        Int
}